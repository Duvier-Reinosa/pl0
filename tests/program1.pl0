program Pi_Spigot;
var n, len, i, j, k, q, x, nines, predigit : int;
a : array[1..len] of int;
fun multiply(a int, b int) locals result int begin
    result := 0;
    while b > 0 do begin
        if b % 2 = 1 then result := result + a;
        a := a * 2;
        b := b / 2;
    end;
    return result;
end
fun print_zeros(nines int) locals k int begin
    for k := 1 to nines do
        write(0);
end
fun main() begin
    n := 1000;
    len := 108 * n / 3;
    
    for j := 1 to len do
        a[j] := 2;
    
    nines := 0;
    predigit := 0;
    
    for j := 1 to n do begin
        q := 0;
        
        for i := len downto 1 do begin
            x := 10 * a[i] + q * i;
            a[i] := x - multiply(2 * i - 1, (x / (2 * i - 1)));
            q := x / (2 * i - 1);
        end;
        
        a[1] := q % 10;
        q := q / 10;
        
        if q = 9 then
            nines := nines + 1;
        else if q = 10 then begin
            write(predigit + 1);
            print_zeros(nines);
            predigit := 0;
            nines := 0;
        end
        else begin
            write(predigit);
            predigit := multiply(predigit, q);
            
            if nines <> 0 then begin
                print_zeros(nines);
                nines := q;
            end;
        end;
    end;
    
    write(predigit);
end
begin
    main();
end.
